- if @run && @start_latlng && @end_latlng
  var start_latlng = #{@start_latlng.to_json.try(:html_safe) || []};
  var end_latlng = #{@end_latlng.to_json.try(:html_safe) || []};
  var waypoints = #{@waypoint_latlngs.to_json.try(:html_safe) || []};
  drawUpcomingPath(#{@run.id}, start_latlng, end_latlng, waypoints);

